{
    "name": "sagautam5/laravel-email-blocker",
    "description": "Rule Based email blocking on laravel application",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sagautam5\\EmailBlocker\\": "app/",
            "Sagautam5\\EmailBlocker\\Database\\Migrations\\": "database/migrations/"    
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sagautam5\\EmailBlocker\\Tests\\": "tests/",
            "Sagautam5\\EmailBlocker\\Database\\Factories\\": "database/factories/"
        }
    },
    "authors": [
        {
            "name": "Sagar Gautam"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "class": "Sagautam5\\EmailBlocker",
        "laravel": {
            "providers": [
                "Sagautam5\\EmailBlocker\\Providers\\EmailBlockServiceProvider"            
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "laravel/pint": "^1.26",
        "orchestra/testbench": "^10.8",
        "phpstan/phpstan": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
